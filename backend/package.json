{
  "name": "abtest-backend",
  "version": "1.0.0",
  "description": "A/B Testing Backend API",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "build": "tsc",
    "seed": "npx ts-node scripts/seed.ts",
    "migrate": "npx ts-node scripts/migrate.ts",
